{
  "name": "react-valid8",
  "version": "1.1.0",
  "description": "Validation library for React forms",
  "author": "mikaelrss",
  "license": "MIT",
  "repository": "mikaelrss/react-valid8",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "jsnext:main": "dist/index.es.js",
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "scripts": {
    "test": "cross-env CI=1 react-scripts-ts test --env=jsdom",
    "test:watch": "react-scripts-ts test --env=jsdom",
    "build": "rollup -c",
