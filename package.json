  "dependencies": {
    "classnames": "^2.2.6",
    "is-url": "^1.2.4",
    "moize": "^5.4.3"
  },
  "peerDependencies": {
    "prop-types": "^15.5.4",
    "react": "^15.0.0 || ^16.0.0",
    "react-dom": "^15.0.0 || ^16.0.0"
  },
  "devDependencies": {
    "@svgr/rollup": "4.1.0",
    "@types/classnames": "^2.2.7",
    "@types/is-url": "^1.2.28",
    "@types/jest": "24.0.11",
    "@types/react": "^16.8.10",
    "@types/react-dom": "^16.0.5",
    "babel-core": "^6.26.3",
    "babel-runtime": "^6.26.0",
    "cross-env": "^5.1.4",
    "gh-pages": "2.0.1",
    "prettier": "^1.16.4",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-scripts-ts": "3.1.0",
    "rollup": "1.7.4",
    "rollup-plugin-babel": "4.3.2",
    "rollup-plugin-commonjs": "^9.1.3",
    "rollup-plugin-node-resolve": "4.0.1",
    "rollup-plugin-peer-deps-external": "^2.2.0",
    "rollup-plugin-postcss": "2.0.3",
    "rollup-plugin-typescript2": "0.20.1",
    "rollup-plugin-url": "2.2.0",
    "typescript": "3.4.1"
  },
  "files": [
    "dist"
  ]
